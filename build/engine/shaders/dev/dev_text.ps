#include <struct.h>

//##########################################################################

SamplerState g_sSampler :register(s0);

Texture2D g_txDiffuse :register(t0);

//##########################################################################

float4 main(VSO_DevText IN):COLOR0
{
	float4 vFinalColor = g_txDiffuse.Sample(g_sSampler, IN.vTexUV.xy);
	// return(vFinalColor * IN.vColor + float4(0.0f, 0.5f, 0.0f, 0.5f));
	return(vFinalColor * IN.vColor);
}
